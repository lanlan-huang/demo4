	"scripts": {
		"dev": "node before.dev.js && cross-env runEnv=develop umi dev",
		"dev:no-proxy": "cross-env NO_PROXY=true contextPath= npm run dev",
		"mock-run": "sb-mock run -p 3004 -t ./mock",
		"mock-build": "sb-mock build -t ./mock",
		"dev-mock": "concurrently -p \"[{name}]\" -n \"mock,roadhog\" -c \"bgMagenta.bold,bgBlue.bold\" \"npm run mock-run\" \"npm run dev\"",
		"lint": "eslint --fix --ext .js src",
		"build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 contextPath= NO_PROXY=true runEnv=demo umi build",
		"build-test": "cross-env NODE_OPTIONS=--max-old-space-size=4096 contextPath= NO_PROXY=true runEnv=test umi build",
		"build-integrate": "cross-env NODE_OPTIONS=--max-old-space-size=4096 contextPath= NO_PROXY=true runEnv=integrate umi build",
		"test": "jest --config jest.json",
		"precommit": "lint-staged",
		"postinstall": "sf mfsu-build"
	},
	"lint-staged": {
		"{src,mock,__tests__}/**/*.{js,json,css,md,less}": [
			"prettier --write",
			"git add"
		]
	}
